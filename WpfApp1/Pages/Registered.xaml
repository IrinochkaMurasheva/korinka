        <Label Content="Окно регистрации" HorizontalAlignment="Center" Margin="0,50,0,0" VerticalAlignment="Top"/>
        <Button Name="Seller" Content="Продавец" HorizontalAlignment="Center" Margin="0,192,0,0" VerticalAlignment="Top" Width="144" Click="Seller_Click"/>
        <Button Name="Return" Content="Назад" HorizontalAlignment="Center" Margin="0,349,0,0" VerticalAlignment="Top" Width="144" Click="Return_Click"/>

    </Grid>
</Page>
